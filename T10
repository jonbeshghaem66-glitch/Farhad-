  <script>
    // Ø¨Ø§Ø²/Ø¨Ø³ØªÙ‡ Ø´Ø¯Ù† Ù¾Ù†Ù„â€ŒÙ‡Ø§
    document.querySelectorAll('.panel').forEach(panel => {
      const header = panel.querySelector('.panel-header');
      header.addEventListener('click', () => {
        panel.classList.toggle('open');
      });
    });

    // Ø§Ø³Ú©Ø±ÙˆÙ„ Ù†Ø±Ù… Ø§Ø² Ù†ÙˆØ§Ø± Ø¢ÛŒÚ©ÙˆÙ†â€ŒÙ‡Ø§
    document.querySelectorAll('.icon-btn').forEach(btn => {
      btn.addEventListener('click', () => {
        const targetSelector = btn.getAttribute('data-scroll');
        const target = document.querySelector(targetSelector);
        if (target) {
          target.scrollIntoView({ behavior: 'smooth', block: 'start' });
          if (target.classList.contains('panel') && !target.classList.contains('open')) {
            target.classList.add('open');
          }
        }
      });
    });

    // Ù†ÙˆØ§Ø± Ù†ÛŒØª Ø±ÙˆØ²Ø§Ù†Ù‡: ÙˆÙ‚ØªÛŒ ØªÛŒÚ© Ø²Ø¯Ù‡ Ø´ÙˆØ¯ØŒ Ù¾ÛŒØ§Ù… ØªØ´Ú©Ø± Ù†Ø´Ø§Ù† Ø¯Ø§Ø¯Ù‡ Ù…ÛŒâ€ŒØ´ÙˆØ¯
    function handleNiyatCheck(checkbox) {
      const thanks = document.getElementById('niyatThanks');
      if (checkbox.checked) {
        thanks.style.opacity = '1';
      }
    }

    // Ø±ÙˆØ²Ø´Ù…Ø§Ø± ØªØ§ Ø³Ø§Ù„Ú¯Ø±Ø¯ Ø¨Ø¹Ø¯ÛŒ Ø´Ù‡Ø§Ø¯Øª
    function updateCountdown() {
      const today = new Date();

      // ØªØ§Ø±ÛŒØ® Ø³Ø§Ù„Ú¯Ø±Ø¯ Ø´Ù‡Ø§Ø¯Øª Ø¨Ù‡ Ù…ÛŒÙ„Ø§Ø¯ÛŒ (ØªÙ‚Ø±ÛŒØ¨ÛŒ: 12 Ú˜Ø§Ù†ÙˆÛŒÙ‡)
      const anniversaryMonth = 0; // Ú˜Ø§Ù†ÙˆÛŒÙ‡ = 0
      const anniversaryDay = 12;  // 12 Ú˜Ø§Ù†ÙˆÛŒÙ‡

      let target = new Date(today.getFullYear(), anniversaryMonth, anniversaryDay);

      // Ø§Ú¯Ø± Ø³Ø§Ù„Ú¯Ø±Ø¯ Ø§Ù…Ø³Ø§Ù„ Ú¯Ø°Ø´ØªÙ‡ØŒ Ø¨Ø±Ùˆ Ø³Ø§Ù„ Ø¨Ø¹Ø¯
      if (target < today) {
        target = new Date(today.getFullYear() + 1, anniversaryMonth, anniversaryDay);
      }

      const diffMs = target - today;
      const diffDays = Math.ceil(diffMs / (1000 * 60 * 60 * 24));

      const el = document.getElementById('countdownDays');
      if (el) {
        el.textContent = diffDays;
      }
    }

    updateCountdown();
    // Ù‡Ø± ÛŒÚ© Ø³Ø§Ø¹Øª ÛŒÚ© Ø¨Ø§Ø± Ø¢Ù¾Ø¯ÛŒØª Ø´ÙˆØ¯ (Ú©Ø§ÙÛŒ Ø§Ø³Øª)
    setInterval(updateCountdown, 1000 * 60 * 60);
  </script>
